<plugin>

<extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            id="x.contribution1">
         <menu
               label="Accounts"
               path="additions"
               id="x.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="Create Accounts Data"
               class="accounts.plugin.ui.actions.CreateAccountData"
               menubarPath="x.menu1/group1"
               enablesFor="1"
               id="x.newAction">
         </action>
      </objectContribution>
   </extension>   
