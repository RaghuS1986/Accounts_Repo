   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
    <extension
         point="org.eclipse.ui.editors">
      <editor
            class="accounts.plugin.ui.editors.AccountsEditor"
            default="true"
            extensions=".data"
            id="accounts.plugin.ui.editors.AccountsEditor"
            name="Accounts">
         <contentTypeBinding
               contentTypeId="accounts.plugin.ui.editors.accountsEditor">
         </contentTypeBinding>
      </editor>
      <editor
            class="accounts.plugin.ui.editors.patti.PattiEditor"
            default="true"
            extensions=".patti"
            id="accounts.plugin.ui.editors.patti.PattiEditor"
            name="Patti">
         <contentTypeBinding
               contentTypeId="accounts.plugin.ui.editors.patti.PattiEditor">
         </contentTypeBinding>
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            file-extensions="data"
            file-names="*"
            id="accounts.plugin.ui.editors.accountsEditor"
            name="Accounts Editor"
            priority="normal">
      </content-type>
      <content-type
            file-extensions="patti"
            file-names="*"
            id="accounts.plugin.ui.editors.patti.PattiEditor"
            name="Patti Editor"
            priority="normal">
      </content-type>
